    "key-did-resolver": "^3.0.0",
    "ky": "^1.2.0",
    "map.groupby": "^1.0.1",
    "multiformats": "^12.1.3",
    "oazapfts": "^5.1.4",
    "object.groupby": "^1.0.2",
    "process": "^0.11.10",
    "regexp.escape": "^1.1.0",
    "runes2": "^1.1.3",
    "satori": "^0.10.13",
    "satori-html": "^0.3.2",
    "sharp": "^0.33.2",
    "svelte-apollo": "^0.5.0",
    "svelte-meta-tags": "^3.1.0",
    "svelvet": "^6.0.12",
