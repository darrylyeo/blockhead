{
  "name": "blockhead",
  "version": "0.0.1",
  "homepage": "https://blockhead.info",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:static": "SVELTE_BUILD_STATIC=1 vite build --mode static",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "generate": "npx npm-run-all generate:*",
    "generate:edgedb:fireblocks-ncw": "pnpx @edgedb/generate edgeql-js --target ts",
    "generate:swagger-typescript-api:zapper": "swagger-typescript-api --path https://api.zapper.fi/api/json --output ./src/api/zapper/api/ --clean-output true --responses true --union-enums true --add-readonly true --route-types false --enum-names-as-values true --extract-request-params true --extract-request-body true --modular true --unwrap-response-data true --single-http-client true --default-response unknown",
    "generate:swagger-typescript-api:moralis": "swagger-typescript-api --path https://deep-index.moralis.io/api-docs/v2/swagger.json --output ./src/api/moralis/api/ --clean-output true --union-enums false --route-types true --enum-names-as-values true --extract-request-params true --extract-request-body true --single-http-client true --default-response {} --unwrap-response-data true",
    "generate:swagger-typescript-api:nftport": "swagger-typescript-api --path https://api.nftport.xyz/openapi.json --output ./src/api/nftport/api/ --clean-output true --union-enums false --route-types true --enum-names-as-values true --extract-request-params true --extract-request-body true --single-http-client true --default-response {} --unwrap-response-data true",
    "generate:houdini": "houdini generate --pull-schema",
    "migrate:edgedb:fireblocks-ncw": "edgedb migration create; edgedb migrate; pnpm run generate:edgedb:fireblocks-ncw",
    "clean:houdini": "rm -rf './$houdini'; rm ./src/**/*.graphql"
  },
  "type": "module",
  "packageManager": "pnpm@8.6.3",
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@coinbase/wallet-sdk": "^3.7.1",
    "@fireblocks/fireblocks-web3-provider": "^1.2.3",
    "@fireblocks/ncw-js-sdk": "^2.0.0",
    "@fontsource/fira-code": "^5.0.5",
    "@fontsource/ubuntu": "^5.0.5",
    "@helia/unixfs": "^1.4.0",
    "@liquality/wallet-sdk": "^2.5.2",
    "@pushprotocol/restapi": "^1.4.5",
    "@pushprotocol/socket": "^0.5.1",
    "@rize-labs/banana-wallet-sdk": "^0.1.25",
    "@tanstack/query-broadcast-client-experimental": "^4.32.6",
    "@tanstack/query-persist-client-core": "^4.32.6",
    "@tanstack/svelte-query": "^4.32.6",
    "@trpc/client": "^10.38.1",
    "@trpc/server": "^10.38.1",
    "@urql/svelte": "^4.0.2",
    "@walletconnect/core": "^2.9.0",
    "@walletconnect/ethereum-provider": "^2.9.0",
    "@walletconnect/jsonrpc-ws-connection": "^1.0.13",
    "@walletconnect/relay-api": "^1.0.9",
    "@walletconnect/sign-client": "^2.9.0",
    "@walletconnect/types": "^2.9.0",
    "@walletconnect/web3-provider": "^1.8.0",
    "@walletconnect/web3wallet": "^1.8.6",
    "@web3modal/standalone": "^2.4.3",
    "abitype": "^0.9.8",
    "array-from-async": "^3.0.0",
    "array.prototype.group": "^1.1.2",
    "array.prototype.grouptomap": "^1.1.2",
    "bip44-constants": "^228.0.0",
    "blockstore-core": "^4.2.0",
    "blockstore-idb": "^1.1.1",
    "buffer": "^6.0.3",
    "datastore-core": "^9.2.0",
    "datastore-idb": "^2.1.2",
    "detect-content-type": "^1.2.0",
    "echarts": "^5.4.2",
    "edgedb": "^1.3.5",
    "ethers": "^6.6.2",
    "etherspot": "^1.43.7",
    "file-type": "^18.5.0",
    "fireblocks-sdk": "^5.6.1",
    "graphql": "^16.7.1",
    "helia": "^1.3.5",
    "idb-keyval": "^6.2.1",
    "key-did-resolver": "^3.0.0",
    "ky": "^0.33.3",
    "multiformats": "^12.0.1",
    "process": "^0.11.10",
    "runes2": "^1.1.2",
    "svelte-apollo": "^0.5.0",
    "svelte-meta-tags": "^2.8.0",
    "svelvet": "^6.0.12",
    "trpc-sveltekit": "^3.5.20",
    "uint8arrays": "^4.0.4",
    "viem": "^1.8.1",
    "webmax": "^0.2.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.3.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@playwright/test": "^1.35.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.22.4",
    "@total-typescript/ts-reset": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "houdini": "^1.2.8",
    "houdini-svelte": "^1.2.8",
    "pnpm": "^8.6.6",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.1.2",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "swagger-typescript-api": "^12.0.4",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "pnpm": {
    "patchedDependencies": {
      "@pushprotocol/restapi@1.4.5": "patches/@pushprotocol__restapi@1.4.5.patch"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  }
}