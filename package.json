    "swagger-typescript-api": "^13.0.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^1.2.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "@didtools/pkh-ethereum@0.4.1": "patches/@didtools__pkh-ethereum@0.4.1.patch"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
